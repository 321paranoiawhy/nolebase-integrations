{
  "extends": "../../tsconfig.json",
  "exclude": [
    "**/dist/**",
    "./uno.config.ts",
    "./vite.config.ts"
  ],
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.vue",
    "**/*.tsx"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": false,
    "declaration": false,
    "composite": false,
    "sourceMap": true,
    "outDir": "./dist"
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
